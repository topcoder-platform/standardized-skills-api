							"mode": "raw",
							"raw": "{\n    \"grant_type\": \"client_credentials\",\n    \"client_id\": \"{{client_id}}\",\n    \"client_secret\": \"{{client_secret}}\",\n    \"audience\": \"https://m2m.topcoder-dev.com/\",\n    \"auth0_url\": \"https://topcoder-dev.auth0.com/oauth/token\"\n}",
