{
    "name": "standardized-skills-api",
    "version": "1.0.0",
    "description": "API endpoints for accessing the new standardized skills across Topcoder",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "start": "node dist/main.js",
        "start:dev": "ts-node-dev --respawn --transpileOnly src/main.ts",
        "lint": "eslint -c .eslintrc 'src/**/*.ts'",
        "format": "eslint -c .eslintrc 'src/**/*.ts' --fix",
        "pretty": "prettier --write \"src/**/*.ts\"",
        "es-init": "node dist/scripts/init-es.js",
        "es-init-force": "node dist/scripts/init-es.js force",
        "load-data-es": "node dist/scripts/load-data-es.js",
        "services:up": "cd ./local && docker-compose up -d",
        "prisma:generate": "prisma generate",
        "prisma:migrate:deploy": "prisma migrate deploy",
        "prisma:studio": "prisma studio"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.2.1",
        "@nestjs/schematics": "^10.0.1",
        "@nestjs/testing": "^10.2.7",
        "@tsconfig/node16": "^16.1.1",
        "@types/express": "^4.17.21",
        "@types/jsonfile": "^6.1.0",
        "@types/lodash": "^4.14.199",
        "@types/multer": "^1.4.7",
        "@types/node": "^17.0.21",
        "@types/superagent": "^4.1.20",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "eslint": "^8.10.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.28.1",
        "prettier": "^2.5.1",
        "ts-node-dev": "^2.0.0",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@nestjs/common": "^10.2.7",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.2.7",
        "@nestjs/platform-express": "^10.2.7",
        "@prisma/client": "^5.9.1",
        "@opensearch-project/opensearch": "^2.2.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cors": "^2.8.5",
        "dayjs": "^1.11.10",
        "dotenv": "^16.0.0",
        "elasticsearch6": "npm:@elastic/elasticsearch@^6.8.8",
        "elasticsearch7": "npm:@elastic/elasticsearch@7.9.1",
        "http-status-codes": "^2.2.0",
        "jsonfile": "^6.1.0",
        "lodash": "^4.17.21",
        "pg": "^8.7.3",
        "prisma": "^5.9.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "superagent": "^8.1.2",
        "tc-core-library-js": "github:appirio-tech/tc-core-library-js#v3.0.1",
        "winston": "^3.7.2"
    }
}
