{
  "name": "standardized-skills-api",
  "version": "1.0.0",
  "packageManager": "pnpm@9.6.0",
  "main": "dist/main.js",
  "description": "API endpoints for accessing the new standardized skills across Topcoder",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "lint": "eslint -c .eslintrc 'src/**/*.ts'",
    "format": "eslint -c .eslintrc 'src/**/*.ts' --fix",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "services:up": "cd ./local && docker-compose up -d",
    "prisma:generate": "pnpm exec prisma generate",
    "prisma:migrate:deploy": "pnpm exec prisma migrate deploy",
    "prisma:studio": "pnpm exec prisma studio"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.8",
    "@types/cls-hooked": "^4.3.9",
    "@types/express": "^4.17.25",
    "@types/lodash": "^4.17.21",
    "@types/node": "^20.19.27",
    "@types/sequelize": "^4.28.20",
    "@types/superagent": "^4.1.24",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.6.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.20",
    "@nestjs/platform-express": "^10.4.20",
    "@prisma/client": "^6.19.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cls-hooked": "^4.2.2",
    "dotenv": "^16.6.1",
    "http-status-codes": "^2.3.0",
    "lodash": "^4.17.21",
    "pg": "^8.16.3",
    "prisma": "^6.19.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.2",
    "sequelize": "^6.37.7",
    "superagent": "^8.1.2",
    "tc-core-library-js": "github:topcoder-platform/tc-core-library-js#master",
    "winston": "^3.19.0"
  }
}
